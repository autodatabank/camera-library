<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >
    <View
        android:id="@+id/adb_cameralibrary_view_unused_area_top"
        android:layout_width="0dp"
        android:layout_height="110dp"
        android:background="@color/adb_cameralibrary_bg_camera_unused_area"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />
    <View
        android:id="@+id/adb_cameralibrary_view_unused_area_bottom"
        android:layout_width="0dp"
        android:layout_height="110dp"
        android:background="@color/adb_cameralibrary_bg_camera_unused_area"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />
    <View
        android:id="@+id/adb_cameralibrary_view_unused_area_start"
        android:layout_width="110dp"
        android:layout_height="0dp"
        android:background="@color/adb_cameralibrary_bg_camera_unused_area"
        app:layout_constraintBottom_toTopOf="@+id/adb_cameralibrary_view_unused_area_bottom"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/adb_cameralibrary_view_unused_area_top"
        />
    <View
        android:id="@+id/adb_cameralibrary_view_unused_area_end"
        android:layout_width="110dp"
        android:layout_height="0dp"
        android:background="@color/adb_cameralibrary_bg_camera_unused_area"
        app:layout_constraintBottom_toTopOf="@+id/adb_cameralibrary_view_unused_area_bottom"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/adb_cameralibrary_view_unused_area_top"
        />
</androidx.constraintlayout.widget.ConstraintLayout>